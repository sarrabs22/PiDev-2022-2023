 {% extends 'backbase.html.twig' %} 

{% block title %}Calendar1{% endblock %}

{% block body %}
    <h1>Calendar1</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ calendar1.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ calendar1.title }}</td>
            </tr>
            <tr>
                <th>Start</th>
                <td>{{ calendar1.start ? calendar1.start|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>End</th>
                <td>{{ calendar1.end ? calendar1.end|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ calendar1.description }}</td>
            </tr>
            <tr>
                <th>AllDay</th>
                <td>{{ calendar1.allDay ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>BorderColor</th>
                <td>{{ calendar1.borderColor }}</td>
            </tr>
            <tr>
                <th>BackgroundColor</th>
                <td>{{ calendar1.backgroundColor }}</td>
            </tr>
            <tr>
                <th>TextColor</th>
                <td>{{ calendar1.textColor }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_calendar1_index') }}">back to list</a>

    <a href="{{ path('app_calendar1_edit', {'id': calendar1.id}) }}">edit</a>

    {{ include('calendar1/_delete_form.html.twig') }}
{% endblock %}
